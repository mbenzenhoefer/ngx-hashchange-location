{
  "version": "0.0.1",
  "name": "ngx-hashchange-location",
  "description": "Trigger hashchange event when routing with HashLocationStrategy",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "node node_modules/.bin/tsc",
    "build:watch": "node node_modules/.bin/tsc -w",
    "code:format": "node node_modules/.bin/prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "code:lint": "node node_modules/.bin/tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run code:lint",
    "pack": "npm pack"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "node",
    "license",
    "check"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mbenzenhoefer/ngx-hashchange-location.git"
  },
  "license": "MIT",
  "author": "Moritz Benzenhoefer",
  "dependencies": {
    "@angular/common": "~7.2.0",
    "@angular/core": "~7.2.0",
    "rxjs": "~6.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/node": "^10.12.21",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
